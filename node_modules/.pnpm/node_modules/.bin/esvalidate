#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
