#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules/uglify-js/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules/uglify-js/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules/uglify-js/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules/uglify-js/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/uglify-js@3.17.4/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
