#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/bin/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules/sshpk/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/sshpk@1.17.0/node_modules:/Users/chlbri/Documents/github/NODE JS/Librairies bemedev/xstate-xstate-4.37.2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/sshpk-conv" "$@"
else
  exec node  "$basedir/../../bin/sshpk-conv" "$@"
fi
